datasource db {
  provider = "mysql"
  // Hapus baris url = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model FileMetadata {
  id             Int      @id @default(autoincrement())
  wallet_address String
  file_name      String
  ipfs_cid       String
  created_at     DateTime @default(now())
}